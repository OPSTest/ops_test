{
  "build": {
    "content": [
      {
        "files": [ 
          "**/*.md",
          "**/*.yml"
        ],
        "src": "version1.0-src",
        "version": "netstandard-1.0",
        "exclude": [
          "**/includes/**"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "version1.1-src",
        "version": "netstandard-1.1",
        "exclude":[
          "**/includes/**"
        ]
      }
      {
        "files": ["**/*.yml", "**/*.md"],
        "src": "ops_test"
      }
    ],
    "version":{
      "netstandard-1.0": {
        "dest": "netstandard-1.0-dest"
      },
      "netstandard-1.1": {
        "dest":"netstandard-1.1-dest"
      }
    },
    "dest": "test_e2e"
  }
}
