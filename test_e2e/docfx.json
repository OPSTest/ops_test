{
  "build": {
    "content": [
      {
        "files": [ 
          "**/*.md",
          "**/*.yml"
        ],
        "src": "version1.0-src",
        "version": "netstandard-1.0",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"

        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "version1.1-src",
        "version": "netstandard-1.1",
        "exclude":[
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      },
      {
        "files": ["**/*.yml", "**/*.md"],
        "src": "ops_test"
      },
       {
        "files": ["*.md", "*.yml"]
       }
    ],
    "versions":{
      "netstandard-1.0": {
        "dest": "netstandard-1.0-dest"
      },
      "netstandard-1.1": {
        "dest":"netstandard-1.1-dest"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {},
    "template": ["DOCS"],
    "dest": "test_e2e"
  }
}
